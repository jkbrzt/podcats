<?xml version="1.0" encoding="UTF-8"?>
<rss xmlns:itunes="http://www.itunes.com/dtds/podcast-1.0.dtd" version="2.0">
    <channel>
        <title>{{ title }}</title>
        <description>{{ description }}</description>
        <link>{{ link }}</link>
        {% if image_url %}
        <itunes:image href="{{ image_url }}"/>
        {% endif %}
        {{ items }}
    </channel>
</rss>
